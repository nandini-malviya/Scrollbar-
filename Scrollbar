import java.awt.*;  
class Slider{  
Slider(){  
            Frame f= new Frame("Slider");  
            Scrollbar s=new Scrollbar();  
            s.setBounds(100,100,100,100);  
            f.add(s);  
            f.setSize(400,400);  
            f.setLayout(null);  
            f.setVisible(true);  
}  
public static void main(String args[]){  
       new Slider();  
}  
}  
